<?xml version="1.0" encoding="UTF-8"?>
<Machine xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="null"
    xsi:schemaLocation="null My.ecore">
  <etat name="A"
      in="//@transition.0"
      out="//@transition.1 //@transition.2"/>
  <etat name="B"
      in="//@transition.1 //@transition.3"
      out="//@transition.0"/>
  <etat name="Initial"
      in="//@transition.2"/>
  <etat name="Final"
      out="//@transition.3"/>
  <transition
      name="AB"
      source="//@etat.0"
      cible="//@etat.1">
    <callevent
        operation="//@operation.0"/>
  </transition>
  <transition
      name="BA"
      source="//@etat.1"
      cible="//@etat.0">
    <callevent
        operation="//@operation.0"/>
  </transition>
  <transition
      name="IA"
      source="//@etat.2"
      cible="//@etat.0"/>
  <transition
      name="BF"
      source="//@etat.1"
      cible="//@etat.3">
    <callevent
        operation="//@operation.1"/>
  </transition>
  <operation
      name="m"/>
  <operation
      name="exit"/>
</Machine>
